{
  "version": 2,
  "builds": [
    {
      "src": "backend/src/server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "web/package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "backend/src/server.ts"
    },
    {
      "source": "/:path*",
      "destination": "/web/:path*"
    }
  ]
}
